            "previous": "%qSmBT2oRoktsMBZjNV9hwf6hTwnxF5jrJ9+Z6twzFwQ=.sha256",
            "author": "@ZqH7Mctu/7DNInxuwl12ECjfrAKUX2tBLq1rOldNhg0=.ed25519",
